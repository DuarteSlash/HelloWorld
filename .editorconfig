# Configuration file for EditorConfig: http://editorconfig.org
root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true

[*.{txt,md}]
trim_trailing_whitespace = false

[*.properties]
charset = latin1

[*.java]
indent_size = 4
